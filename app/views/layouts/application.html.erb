<!DOCTYPE html>
<html>
<head>
  <title>LastAirBnb</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if logged_in? %>
  Welcome <%= current_user.name %> <%= link_to 'Log Out', session_url(session[:token]), method: :delete %>
<% else %>
  <%= link_to 'Log in', new_session_url %>
  <%= link_to 'Sign up', new_user_url %>
<% end %>

<%= render 'shared/messages' %>

<%= yield %>

</body>
</html>

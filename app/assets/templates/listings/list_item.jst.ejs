<a href="<%= listing.href() %>" data-listing-id='<%= listing.id %>'>
  <div class="img-viewport"
    style="background-image: url(<%= listing.get('cover_photo_url') %>)"
  >
    <div class="carousel-img active" style="background-image: url(<%= listing.escape('cover_photo_url') %>)"></div>
    <% listing.subPhotos().forEach(function (photo){ %>
      <div class="carousel-img" style="background-image: url(<%= photo.escape('url') %>)"></div>
    <% }); %>

    <div class="item-price">
      <%= listing.get('price') %> <span class='yuan'>yuan</span>
    </div>
  </div>
  <div class="item-info-panel">
    <h3><%= listing.escape('title') %></h3>
    <span class="muted"><%= listing.escape('address') %></span>
  </div>
</a>

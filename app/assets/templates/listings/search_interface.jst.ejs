<div class="listings-container">
  <div class="row search-panel">
    <div class="row panel-section">
      <div class="col-md-2">
        <span class="muted">Arrangements</span>
      </div>
      <div class="col-md-10">
        <div class="col-sm-4 row-space-1-sm">
          <input
            id="checkin"
            type="text"
            class="datepicker"
            name="reservation[start_date]"
            value="<%= checkin %>"
            placeholder="Check In"
          >
        </div>

        <div class="col-sm-4 row-space-1-sm">
          <input
            id="checkout"
            type="text"
            class="datepicker"
            name="reservation[end_date]"
            value="<%= checkout %>"
            placeholder="Check Out"
          >
        </div>

        <div class="col-sm-4 row-space-1-sm">
          <select name="reservation[guests]" id="guestcount" placeholder="Guests">
            <% _.times(12, function (n) { %>
              <option
                value="<%= n+1 %>"
                <% if (guests == n+1) { %>
                  selected
                <% } %>
              ><%= n+1 %> <%= pluralize('Guest', n+1) %></option>
            <% }); %>
          </select>
        </div>

      </div>
    </div>
    <div class="row panel-section">
      <div class="col-md-2">
        <span class="muted">Price Range</span>
      </div>
      <div class="col-md-10">
        <div id="price-range"></div>
        <div id="price-min"></div><div id="price-max"></div>
      </div>
    </div>
  </div>
  <div class="row">


    <div class="listings-items">

    </div>
  </div>
</div>
